<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>AddBlogServlet</servlet-name>
    <servlet-class>com.aryanlanghanoja.blog_crud.view.AddBlogServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddBlogServlet</servlet-name>
    <url-pattern>/AddBlogServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ViewBlogsServlet</servlet-name>
    <servlet-class>com.aryanlanghanoja.blog_crud.view.ViewBlogsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewBlogsServlet</servlet-name>
    <url-pattern>/ViewBlogsServlet</url-pattern>
  </servlet-mapping>

</web-app>
